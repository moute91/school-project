pipeline {
  agent any
  stages {
    stage ('tacs') {
      steps{
      echo "how are you doing tacs"
        sleep 4
      }
    }
    stage ('test') {
      steps{
      echo "hello build"
        sleep 4
      }
    }
    stage ('dev') {
      steps{
      echo "how are dev team"
        sleep 4
      }
    }
    stage ('deploy') {
      steps{
      echo "what is deploying"
        sleep 4
      }
    }
  }
}
